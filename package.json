{
  "name": "nuxt-starter-plain-dev",
  "type": "module",
  "private": true,
  "scripts": {
    "------DEFAULT------": "exit 0",
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "------PRETTIER------": "exit 0",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "------ESLINT------": "exit 0",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky"
  },
  "dependencies": {
    "@nuxt/eslint": "1.11.0",
    "eslint": "^9.39.1",
    "nuxt": "^4.2.1",
    "vue": "^3.5.25",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "3.7.2"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
